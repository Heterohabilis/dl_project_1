    "    tokenizer.encode(\"True\", add_special_tokens=False)[-1],  # 5162\n",
    "    tokenizer.encode(\" True\", add_special_tokens=False)[-1] # 1563\n",
    "]\n",
    "false_token_ids = [\n",
    "    tokenizer.encode(\"False\", add_special_tokens=False)[-1], # 7700\n",
    "    tokenizer.encode(\" False\", add_special_tokens=False)[-1] # 2137\n",
    "]\n",
    "\n",
    "print(f\"Token IDs - True: {true_token_ids}, False: {false_token_ids}\")\n",
    "\n",
    "model_predictions = []\n",
    "ground_truths = []\n",
    "\n",
    "print(f\"Running inference on {num_samples} random validation samples using LOGITS...\")\n",
    "\n",
    "# Á°Æ‰øùÊ®°ÂûãÂú®ËØÑ‰º∞Ê®°Âºè\n",
    "model.eval()\n",
    "with torch.no_grad():\n",
    "    for example in tqdm(random_samples):\n",
    "        question = clean_text(example[\"question\"])\n",
    "        solution = clean_text(example[\"solution\"])\n",
    "        correct_answer = example[\"is_correct\"]\n",
    "\n",
    "        prompt = inference_prompt.format(question, str(solution))\n",
    "        # Ê≥®ÊÑèÔºöËøôÈáåÊàë‰ª¨‰∏çÈúÄË¶Å [prompt] ÂàóË°®ÔºåÂõ†‰∏∫Êàë‰ª¨‰∏ÄÊ¨°Âè™Â§ÑÁêÜ‰∏Ä‰∏™\n",
    "        inputs = tokenizer(prompt, return_tensors=\"pt\").to(\"cuda\")\n",
    "\n",
    "        # Áõ¥Êé•Ëé∑ÂèñÊ®°ÂûãËæìÂá∫ÔºåËÄå‰∏çÊòØÁîüÊàê\n",
    "        outputs = model(**inputs)\n",
    "        logits = outputs.logits\n",
    "\n",
    "        # Ëé∑ÂèñÊúÄÂêé‰∏Ä‰∏™ token (Âç≥ \"Output:\\n\" ‰πãÂêéÁöÑÈÇ£‰∏™) ÁöÑ logits\n",
    "        # logits ÂΩ¢Áä∂: [batch_size, sequence_length, vocab_size]\n",
    "        last_token_logits = logits[0, -1, :]\n",
    "\n",
    "        # ÊØîËæÉ \"True\" Âíå \"False\" token ÁöÑÂàÜÊï∞\n",
    "        # Êàë‰ª¨ÂèñÊâÄÊúâ True/False Âèò‰Ωì‰∏≠ÁöÑÊúÄÈ´òÂàÜ\n",
    "        true_score = torch.max(last_token_logits[true_token_ids])\n",
    "        false_score = torch.max(last_token_logits[false_token_ids])\n",
    "\n",
    "        prediction = true_score > false_score\n",
    "        \n",
    "        model_predictions.append(prediction.item())\n",
    "        ground_truths.append(correct_answer)\n",
    "\n",
    "accuracy = calculate_accuracy(ground_truths, model_predictions)\n",
    "print(f\"\\nCalculation complete.\")\n",
    "print(f\"Validation Accuracy (Logits Method) on {num_samples} random samples: {accuracy * 100:.2f}%\")"
   ]
  },
